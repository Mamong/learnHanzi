/**app.wxss**/
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}


page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.bold{
  font-weight: 500;
}

.warn{
  color: rgb(241, 64, 64);
}

.plain-button {
  padding: 12rpx 12rpx;
  color: #fff;
  background-color: #07c160;
  border-radius: 12rpx;
  font-size: 28rpx;
}

.outline-button {
  padding: 12rpx 12rpx;
  color: #07c160;
  background-color: #fff;
  border: 4rpx solid #07c160;
  border-radius: 12rpx;
  font-size: 24rpx;
}

.pyfont {
  font-family: pinyinfont;
}

@font-face {
  font-family:pinyinfont;font-style:normal;font-weight:400;src:url(data:font/truetype;charset=utf-8;base64,) format("truetype");
}

.kt {
  font-family: kt;
}

@font-face {
  font-family:kt;font-style:normal;font-weight:400;src:url("https://pinyin-1300966116.file.myqcloud.com/kaiti_sub.woff2");
}

.iconfont {
  font-family: iconfont;
}

@font-face {
  font-family: 'iconfont';
  src: url('data:font/ttf;charset=utf-8;base64,') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

.lesson .py {
  background: #fff;
  border-bottom: 2rpx solid #e0e0e0;
  border-top: 2rpx solid #e0e0e0;
  color: #999;
  height: 58rpx;
  text-align: center;
}

.lesson .py .pyfont {
  font-size: 40rpx;
  letter-spacing: -2rpx;
  line-height: 52rpx;
  text-align: center;
  z-index: 2;
}

.lesson_true .char .kt,.lesson_true .py .pyfont,.lesson_true .title,.wordBox_true .char .kt,.wordBox_true .py .pyfont,.wordBox_true .title {
  color: #333;
}

.lesson .py .pyfont,.lesson .py::before {
  display: block;
  position: absolute;
  width: 122rpx;
}

.lesson .py::before {
  height: 18rpx;
}

.lesson .py::after,.lesson .py::before {
  border-bottom: 2rpx dotted #e0e0e0;
  content: " ";
}

.lesson .py::after {
  display: block;
  height: 38rpx;
  position: absolute;
  width: 122rpx;
}

